generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model todos {
  id          Int      @id @unique
  user_id     String   @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  task        String?
  is_complete Boolean? @default(false)
  inserted_at DateTime @default(dbgenerated("timezone('utc'::text, now())")) @db.Timestamptz(6)
  category    String?
}
